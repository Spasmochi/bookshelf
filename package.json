{
  "name": "gatsby-starter-typescript-deluxe",
  "description": "A Gatsby starter with TypeScript, Storybook, Styled Components, Jest, and more.",
  "version": "0.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe"
  },
  "bugs": {
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:storybook": "build-storybook",
    "update": "ncu -u"
  },
  "dependencies": {
    "framer-motion": "^2.6.1",
    "gatsby": "^2.24.50",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.24",
    "gatsby-plugin-offline": "^3.2.24",
    "gatsby-plugin-react-axe": "^0.5.0",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.28",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-source-filesystem": "^2.3.25",
    "gatsby-transformer-sharp": "^2.5.13",
    "modern-css-reset": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@storybook/addon-actions": "^6.0.18",
    "@storybook/addon-knobs": "^6.0.18",
    "@storybook/addon-links": "^6.0.18",
    "@storybook/addons": "^6.0.18",
    "@storybook/react": "^6.0.18",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-gatsby": "^0.5.6",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.7.0",
    "eslint-config-gojutin": "0.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-watch": "^7.0.0",
    "gatsby-plugin-emotion": "^4.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "npm-check-updates": "^7.1.1",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "tslint-immutable": "^6.0.1",
    "twin.macro": "^1.7.0",
    "typescript": "^4.0.2"
